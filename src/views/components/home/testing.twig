<section class="block">
  <div class="flex flex-wrap gap-4">
    <h2>Tesing component</h2>
    <h2>{{ label }}</h2>
    {% for image in component.testingid %}
    <div>
      <p>
        {{ image.description }}
      </p>
      <img src="{{ image.value }}" alt="{{ image.label | default('Image') }}" />
    </div>
    {% endfor %}
  </div>
</section>
